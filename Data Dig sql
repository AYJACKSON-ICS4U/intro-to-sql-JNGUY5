SELECT name FROM people_age_height WHERE AGE>15 OR HEIGHTcm>153;

SELECT name, sum(AGE) AS TOTAL_AGE FROM people_age_height
    GROUP BY name
    HAVING TOTAL_AGE > 55;
    
